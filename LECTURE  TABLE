
CREATE TABLE LECTURER(
    NIC      VARCHAR(20),
    LecID    VARCHAR(20),
    Position VARCHAR(50),
    F_Name   VARCHAR(50),
    L_Name   VARCHAR(50),
    Gender   BOOLEAN,
    Email    VARCHAR(50),
    DepID    VARCHAR(20),
    PRIMARY KEY (NIC),
    FOREIGN KEY (DepID) REFERENCES DEPARTMENT(DepID),
    FOREIGN KEY (NIC) REFERENCES USER();
    

);
