CREATE TABLE(
   Quiz_01 DECIMAL(3,2),
   Quiz_02 DECIMAL(3,2),
   Quiz_03 DECIMAL(3,2),
   Assesment INTEGER,
   Mid_Practical INTEGER,
   Mid_Theory INTEGER,
   End_Practical INTEGER,
   End_Theory INTEGER,
   Stu_ID VARCHAR(20),
   CourseID VARCHAR(20),
   DepID VARCHAR(20),
   FOREIGN KEY (DepID) REFERENCES DEPARTMENT(DepID)
);