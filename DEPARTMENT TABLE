CREATE TABLE DEPARTMENT(
     Dep_ID  VARCHAR(20),
     D_Name VARCHAR(50),
     DEAN_ID VARCHAR(20),
     ADMIN_ID VARCHAR(20),
     PRIMARY KEY(Dep_ID),
     FOREIGN KEY(DEAN_ID) REFERENCES DEAN(DEAN_ID),
     -- FOREIGN KEY(ADMIN_ID) REFERENCES ADMIN(ADMIN_ID)
);

INSERT INTO Department(Dep_ID, D_Name, DEAN_ID)
VALUES 
('D_ICT', 'ICT Department', 'D001'),
('D_ET', 'ET Department', 'D001'),
('D_MDS' , 'Multideciplinary Department','D001');

